!#groovy


properties([dasableConcurrentBuilds()])

pipeline {
    agent {
        label 'master'
    }
    options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
        timstamps()
    }
    stages {
        stage("First step") {
            steps {
                sh 'ssh root@slave \'hostname\''
            }
        }
    }
}
